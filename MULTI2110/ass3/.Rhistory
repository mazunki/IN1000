survey_data <- read.csv("survey.csv", header=TRUE)
means <- list()
std <- list()
means$words <- mean(survey_data$words)
std$words <- sd(survey_data$words)
means$exp <- mean(survey_data$stats_xp)
std$exp <- sd(survey_data$stats_xp)
print(paste("Mean of words:", round(means$words, 2)))
print(paste("Standard deviation of words:", round(std$words, 2)))
print(paste("Mean of stats_xp:", round(means$exp, 2)))
print(paste("Standard deviation of stats_xp:", round(std$exp, 2)))
View(means)
