WIDTH <- 100
HEIGHT <- 100
coords <- read.table("magic.csv", sep = ",", col.names = c("x", "y"));
img <- matrix(255, nrow = HEIGHT, ncol = WIDTH)
for (i in 1:nrow(coords)) {
img[coords$y[i], coords$x[i]] <- 0
}
jpeg("rplot.png")
plot(img)
